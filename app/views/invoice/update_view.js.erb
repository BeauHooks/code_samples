<% @lines.each do |line| %>
	<% if line.number <= 2299 %>
    $("tr#hud_row_<%= line.number %>").attr("class", "<%= line.type.gsub('_custom', '') %>");
    $("tr#hud_row_<%= line.number %>").html("<%= j(render "huds/partials/full-line", builder: line ).html_safe %>");
	<% else %>
    $("tr#hud_row_<%= line.number %>").attr("class", "<%= line.type.gsub('_custom', '') %>");
    $("tr#hud_row_<%= line.number %>").html("<%= j(render "huds/partials/full-line", builder: line ).html_safe %>");
	<% end %>
<% end %>

