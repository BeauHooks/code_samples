<% case @field_type %>
<% when "Name" %>
  $("#rolodex_name").html("<%= j(render 'rolodex/partials/info_field').html_safe %>");
<% when "Description" %>
  $("#rolodex_description").html("<%= j(render 'rolodex/partials/info_field').html_safe %>");
<% when "IndCorp" %>
  $("#rolodex_indcorp").html("<%= j(render 'rolodex/partials/info_field').html_safe %>");
<% when "CustomerType" %>
  $("#rolodex_customer_type").html("<%= j(render 'rolodex/partials/info_field').html_safe %>");
<% when "SendDocsVia" %>
  $("#rolodex_send_docs_via").html("<%= j(render 'rolodex/partials/info_field').html_safe %>");
<% when "IsActive" %>
  $("#rolodex_is_active").html("<%= j(render 'rolodex/partials/info_field').html_safe %>");
<% when "EffectiveDate" %>
  $("#rolodex_effective_date").html("<%= j(render 'rolodex/partials/info_field').html_safe %>");
<% when "TaxID" %>
  $("#rolodex_tax_id").html("<%= j(render 'rolodex/partials/info_field').html_safe %>");
<% end %>

