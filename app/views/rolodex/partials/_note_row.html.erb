<tr id="BLANK" data-section="note">	<td class="note"><input type="text" /></td>	<td class="from"><%= select :note, :From, Employee.find(:all, conditions: ["IsActive = ?", "True"], order: "FullName ASC").collect{|from| [from.FullName, from.ID]}, { include_blank: false, selected: "#{current_user.employee_id}" }, disabled: "disabled" %></td>	<td class="to"><%= select :note, :To, Employee.find(:all, conditions: ["IsActive = 'True' AND ID NOT IN (190, 119, 95, 157, 327) "], order: "FullName ASC").collect{|to| [to.FullName, to.ID]}, { include_blank: false, selected: "#{current_user.employee_id}" } %></td>	<td class="date"><input id="datepicker" type="text"></td></tr>