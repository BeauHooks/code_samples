<td class="content" colspan="10">  <table>    <% c = 0 %>    <% if rolodex.children.first != nil %>      <% rolodex.children.each do |child| %>        <% c += 1 %>        <tr id="child<%= c %>" onclick="changeRolodex(this.id);">          <td class="expand"><%= link_to "", "rolodex/#{child.EntityID}/show_contact", remote: true %></td>          <td class="name"><div><%=truncate(child.name, length: 20) unless child.name.nil?  %></div></td>          <td class="affiliation"><div><%= truncate(child.primary_affiliation, length: 20) unless child.primary_affiliation.nil? %></div></td>          <% if !child.entity_contacts.phones_callable.first.nil? %>            <td class="phone"><div><%= link_to truncate("#{child.entity_contacts.phones_callable.first.ContactDesc}: <a href='#{child.entity_contacts.phones_callable.first.Contact}'>#{child.entity_contacts.phones_callable.first.Contact}</a>", :length => 25) %></div></td>          <% else %>            <td class="phone"></td>          <% end %>          <td class="email"><div><%= mail_to truncate(child.email, length: 25) %></div></td>          <td class="rolodex_address"><div><%= truncate(child.address[0], length: 35) unless child.address[0].nil? %></div></td>          <td class="city"><div><%= truncate(child.address[1], length: 17) unless child.address[1].nil? %></div></td>          <td class="state" class="center"><%= child.address[2] unless child.address[2].nil? %></td>          <td class="employee"><%= child.employee unless child.employee.nil? %></td>          <td class="files center"><%= child.open_files unless child.is_master? %></td>        </tr>      <% end %>    <% end %>  </table>  <% if c > 0 %>    <script type="text/javascript">      changeRolodex("child1");    </script>  <% end %></td>