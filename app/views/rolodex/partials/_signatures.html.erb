<div id="rolodex_signatures">  <div id="signature_left">    <div id="signatures" class="box">      <div class="box_header">        <p class="title">Signatures <img src="/assets/plus.png" onclick="$.post('/rolodex_signatures/new?entity=<%= @rolodex_entity.EntityID %>');" remote="true" class="icon trigger" /></p>        <div class="tools">        </div>      </div>      <div class="grid_header">        <table>          <tr>            <td class="signature">Signature</td>            <td class="modify_date">Modified</td>            <td class="modify_user">Modified By</td>            <td class="active" id="rolodex_signatures_last">Active</td>          </tr>        </table>      </div>      <div class="results" id="signatures_info">      </div>    </div>    <div id="manage_tree_view" class="box">    <div class="box_header">      <p class="title">Tree View</p>      <div class="tools">      </div>    </div>    <div id="tree_view" class="pos_relative">      <div id="tree">      </div>    </div>  </div></div>  <div id="signature_right">    <div id="signature_preview" class="box">    <div class="box_header">      <p class="title">Signature Preview</p>      <div class="tools" id="signature_tools">          <ul>            <li class="menu_item">              <!--<a class="trigger" href="" data-remote="true" id="format_signature">Signature Format</a>-->            </li>            <li class="menu_item"><a class="trigger" href="signatures//show_editor" data-remote="true" id="edit_signature">Edit</a></li>          </ul>      </div>    </div>    <div id="signature_content" class="doc">    </div>  </div>  </div></div><!-- Overlays --><!-- Add Signature --><div class="overlay" id="overlay_signature"></div>