<% cancel = "addToQueue('cancel_update_file_info','index/#{@file.ID}/cancel_update_file_info');" %><% if @type == "TransactionDescription" %>  <%= select_tag :TransactionType, options_from_collection_for_select(TransactionType.find(:all, order: 'TransactionDescription ASC'), :ID, :TransactionDescription), include_blank: true, onkeyup: "if(event.keyCode == 27){#{cancel}}", data: {url: index_path(@file.ID), method: :put, remote: true} %><% elsif @type == "Underwriter" %>	<%= select_tag :Underwriter, options_from_collection_for_select(Underwriter.find(:all, order: 'Underwriter ASC', conditions: "IsActive = -1"), :ID, :Underwriter), include_blank: true, selected: @file.Underwriter, onkeyup: "if(event.keyCode == 27){#{cancel}}", data: {url: index_path(@file.ID), method: :put, remote: true} %><% elsif @type == "COEDate" %>	<%= text_field_tag :COEDate, @file.COEDate.to_s(:std), onkeyup: "if(event.keyCode == 27){#{cancel}}", class: "datepicker", placeholder: "mm/dd/yyyy", data: {url: index_path(@file.ID), method: :put, remote: true} %><% elsif @type == "SalesPrice" %>	<%= text_field_tag :SalesPrice, number_with_precision(@file.SalesPrice, precision: 2), onkeydown: "numbersOnly(event);",onkeyup: "if(event.keyCode == 27){#{cancel}}", data: {url: index_path(@file.ID), method: :put, remote: true} %><% elsif @type == "LoanAmount" %>	<%= text_field_tag :LoanAmount, number_with_precision(@file.LoanAmount, precision: 2), onkeydown: "numbersOnly(event);", onkeyup: "if(event.keyCode == 27){#{cancel}}", data: {url: index_path(@file.ID), method: :put, remote: true} %><% elsif @type == "SecondLoan" %>	<%= text_field_tag :SecondLoan, number_with_precision(@file.SecondLoan, precision: 2), onkeydown: "numbersOnly(event);", onkeyup: "if(event.keyCode == 27){#{cancel}}", data: {url: index_path(@file.ID), method: :put, remote: true} %><% elsif @type == "LoanNum" %>	<%= text_field_tag :LoanNum, @file.LoanNum, onkeyup: "if(event.keyCode == 27){#{cancel}}", data: {url: index_path(@file.ID), method: :put, remote: true} %><% end %>